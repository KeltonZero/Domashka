#include <iostream>
#include <cmath>

using namespace std;
/*Вводятся три целых числа. Определить какое из них наибольшее.*/
int main() {

	int a , b , c;
	cout << "Enter a , b ,c" << endl;
	cin >> a >> b >> c;
	if (a > b) {
		if (a > c) {
			cout << a << endl;


		}
		else {
			cout << c << endl;
		}
	}
	else {
		if (b > c) {
			cout << b << endl;

		}
		else {
			cout << c << endl;
		}
	}
	

	return 0;
}
